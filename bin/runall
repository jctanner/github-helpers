#!/bin/bash
WEBDIR=/var/www/html/ansible

rm -f $WEBDIR/prs_by_file.html
bin/issues showprfiles --html | egrep -v ^\# | egrep -v 1034h | tee -a $WEBDIR/prs_by_file.html

rm -f $WEBDIR/prs_by_user.html
bin/issues showprusers --html | egrep -v ^\# | egrep -v 1034h | tee -a $WEBDIR/prs_by_user.html

rm -rf $WEBDIR/pr_merge_commits.html
bin/issues showprmergecommits --html | egrep -v ^\# | egrep -v 1034h | tee -a $WEBDIR/pr_merge_commits.html

rm -rf $WEBDIR/opentickets.csv
bin/issues showall | egrep -v 1034h | egrep -v ^\# | tee -a $WEBDIR/opentickets.csv
